{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "dev",
        "type": "shell",
        "options": {
          "cwd": "${workspaceFolder}/cmd/webserver"
        },
        "command": "go",
        "args": [
          "build",
          "-tags",
          "dev",
          "-gcflags",
          "all=-N -l",
          "-o",
          "build/bin/hello.exe"
        ]
      },
      {
        "label": "prod",
        "type": "shell",
        "options": {
          "cwd": "${workspaceFolder}"
        },
        "command": "go",
        "args": [
          "build",
          "-tags",
          "wv2runtime.browser"
        ]
      }
    ]
  }